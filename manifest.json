{
  "manifest_version": 2,
  "name": "Google Meet Push To Talk",
  "version": "0.0.2",
  "description": "Push space to talk instead of fumbling around trying to mute and unmute",
  "icons": {
    "16": "icons/monkey16.png",
    "48": "icons/monkey48.png",
    "128": "icons/monkey128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "ptt.js"
      ]
    }
  ]
}
